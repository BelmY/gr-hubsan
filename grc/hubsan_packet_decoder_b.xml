<?xml version="1.0"?>
<block>
  <name>Hubsan packet decoder</name>
  <key>hubsan_packet_decoder_b</key>
  <category>hubsan</category>
  <import>import hubsan</import>
  <make>hubsan.packet_decoder_b()</make>

  <sink>
    <name>in</name>
